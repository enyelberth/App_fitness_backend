generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}
model Product {
  id        Int      @id @default(autoincrement())
  title     String   @db.VarChar(255)
  price     String   @db.VarChar(255)
  cantidad  Int

}

